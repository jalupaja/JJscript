test = {(a)
    if (a >= 2) {
        return("STOP");
    }
    if (a >= 1) {
        return("THE END");
    }
    "NVM";
}

printl(test(2));
printl(test(1));
printl(test(0));

test2 = {(a, b)
    printl("${a} ${b}");
}

args = [1, 3];
test2(args);
for i : 0:len(args) {
    args[i] += 1;
}
test2(args);
